    "url-join": "^4.0.0"
  },
  "devDependencies": {
    "eol": "^0.9.1",
    "eslint": "^4.5.0",
    "eslint-config-airbnb-base": "^11.3.2",
    "eslint-plugin-import": "^2.17.2",
    "express": "^4.16.4",
    "mkdirp": "^0.5.0",
    "request": "^2.88.0",
    "tap": "^12.7.0"
  },
  "license": "MIT"
